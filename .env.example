# Environment Configuration Template
# Copy this file to .env.production and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# App Configuration
FLUTTER_APP_NAME=Flight Compensation
FLUTTER_APP_VERSION=1.0.0
FLUTTER_ENV=production

# API Configuration  
API_BASE_URL=https://your-domain.com/api
API_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_LOGGING=false

# Security
ENABLE_SSL_PINNING=true
ENABLE_CERTIFICATE_PINNING=true

# PWA Configuration
PWA_SHORT_NAME=FlightClaim
PWA_NAME=Flight Compensation App
PWA_DESCRIPTION=Claim compensation for delayed or cancelled flights
PWA_THEME_COLOR=#1976D2
PWA_BACKGROUND_COLOR=#FFFFFF

# Build Configuration
WEB_RENDERER=canvaskit
TREE_SHAKE_ICONS=true
SPLIT_DEBUG_INFO=true
OBFUSCATE=true